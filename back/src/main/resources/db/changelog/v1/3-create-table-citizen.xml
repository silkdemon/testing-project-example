<?xml version="1.0" encoding="UTF-8" ?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet logicalFilePath="3-create-table-citizen"
               id="3-create-table-citizen" author="maria">
        <createTable tableName="citizen_model3">
            <column name="id" type="serial">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="first_name" type="varchar(100)">
                <constraints nullable="false"/>
            </column>
            <column name="second_name" type="varchar(100)">
                <constraints nullable="false"/>
            </column>
            <column name="passport" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="birthdate" type="datetime">
                <constraints nullable="false"/>
            </column>
            <column name="city" type="varchar(100)">
                <constraints nullable="false"/>
            </column>
            <column name="country" type="varchar(100)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>